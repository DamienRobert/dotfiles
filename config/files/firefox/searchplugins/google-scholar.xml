<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Google Scholar</os:ShortName>
<os:Description>Google Scholar</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/png;base64,</os:Image>
<SearchForm>http://scholar.google.fr/schhp?hl=en&amp;as_sdt=0,5</SearchForm>
<os:Url type="text/html" method="GET" template="http://scholar.google.fr/scholar">
  <os:Param name="q" value="{searchTerms}"/>
  <os:Param name="hl" value="en"/>
  <os:Param name="btnG" value="Search"/>
  <os:Param name="as_sdt" value="1%2C5"/>
  <os:Param name="as_sdtp" value="on"/>
</os:Url>
</SearchPlugin>