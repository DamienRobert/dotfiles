<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Google Translate</os:ShortName>
<os:Description>Google Translate</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/png;base64,</os:Image>
<SearchForm>http://translate.google.com/#auto|en|</SearchForm>
<os:Url type="text/html" method="POST" template="http://translate.google.com/">
  <os:Param name="sl" value="auto"/>
  <os:Param name="tl" value="en"/>
  <os:Param name="js" value="n"/>
  <os:Param name="prev" value="_t"/>
  <os:Param name="hl" value="en"/>
  <os:Param name="ie" value="UTF-8"/>
  <os:Param name="layout" value="2"/>
  <os:Param name="eotf" value="1"/>
  <os:Param name="text" value="{searchTerms}"/>
</os:Url>
</SearchPlugin>