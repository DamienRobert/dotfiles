#compdef git-mypatch

_arguments \
  '-name[name of patch]' \
  '-cc[use git-mycc as cc-cmd]' \
  '-dir[patch directory (11patch-upstream by default)]' \
  '-dirfrom[action directory (tosend/toapply)]' \
  '-dirto[action directory (sent/applied)]' \
  '1:Action:(format send sent toresend check apply applied toreapply)' \
  '*:: :_normal'
