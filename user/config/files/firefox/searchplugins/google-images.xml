<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Google Images</os:ShortName>
<os:Description>Google Images</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/png;base64,</os:Image>
<SearchForm>http://www.google.com/imghp</SearchForm>
<os:Url type="text/html" method="GET" template="http://www.google.com/search">
  <os:Param name="tbm" value="isch"/>
  <os:Param name="hl" value="en"/>
  <os:Param name="source" value="hp"/>
  <os:Param name="biw" value="1019"/>
  <os:Param name="bih" value="475"/>
  <os:Param name="q" value="{searchTerms}"/>
  <os:Param name="btnG" value="Search+Images"/>
  <os:Param name="gbv" value="2"/>
  <os:Param name="oq" value=""/>
  <os:Param name="aq" value=""/>
  <os:Param name="aqi" value=""/>
  <os:Param name="aql" value=""/>
  <os:Param name="gs_sm" value=""/>
  <os:Param name="gs_upl" value=""/>
</os:Url>
</SearchPlugin>