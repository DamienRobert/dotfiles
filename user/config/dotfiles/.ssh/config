NoHostAuthenticationForLocalhost yes
StrictHostKeyChecking no
UserKnownHostsFile /dev/null

# General type of config
# ======================

Host *443
ProxyCommand sshforward %h %p

Host local _local _localhost
HostName localhost
NoHostAuthenticationForLocalhost yes

Host *
TCPKeepAlive yes
#ServerAliveInterval 300
#VerifyHostKeyDNS ask
VerifyHostKeyDNS no
ForwardAgent yes
ForwardX11 no
SendEnv LANG LC_*
SendEnv TZ
HashKnownHosts no
StrictHostKeyChecking ask
ControlPath "~/run/ssh@%l:%r@%h:%p"
#ControlMaster auto
#ControlPersist yes

