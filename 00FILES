cd ~/mine.github
rm -rf bin config doc ruby script syst

cp -r ../mine/{bin,ruby} .
mkdir -p doc/Sc2
cp -r ../mine/doc/{Crypto,Maths,Sc2/Hots,Sc2/Wol,gcc,git,info,logiciels,ruby,unix} doc
mkdir script
cp ../mine/script/*(.) script/
cp -r ../mine/script/{extra,firefox,git,others,pacman,services} script/
mkdir -p syst/{etc,new}
cp -r ../mine/syst/etc/{0*,etc-generate} syst/etc/
cp -r ../mine/syst/new/0* syst/new/
mkdir -p config/{files,.mutt}
cp -r ../config/files/{conkyrc*,firefox} config/files
cp -r ../config/.mutt/{colors,gpg*,muttrc,zhooks} config/.mutt
cp -r ../config/{new,squel,00COMPUTERS.rb} config
cp -r ../config/{.XCompose,.Xclients,.Xkbmap,.Xresources,.biber.conf,.config,.conkyrc-*,.dir_colors,.dmrc,.fvwm,.gitconfig,.gprc,.gvimrc,.hgrc,.i18n,.initenv,.initlog,.inputrc,.latexmkrc,.local-desktop,.mailcap,.makepkg.conf,.mpd,.mplayer,.ncmpcpp,.ore,.pacman.conf,.profile,.pryrc,.screenrc,.vim,.vimrc,.xgeometry,.xinit,.xinitrc,.xinput.d,.xkb,.xprofile,.xserverrc,.xsession,.ytalkrc,.zprofile,.zsh,.zshrc} config/

rm -f syst/etc/00updatecomptefiles
rm -f config/new/00updatecomptefiles
rm -f script/{unlock,get_authinfo,offlineimap_authinfo,checkmail,anniversaire.yaml,extra/*photos*}
rm -f doc/info/Free
